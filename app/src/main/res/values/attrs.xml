<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_padding" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>


    <style name="IGapRootViewStyle">
        <item name="android:background">#1A1A1A</item>
        <item name="android:layoutDirection">locale</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="MySearch" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:editTextColor">#1A1A1A</item>
        <item name="android:textColorHint">#9B9B9B</item>
        <item name="android:tint">#41B120</item>
        <item name="android:gravity">end|right</item>
        <item name="android:textColor">#1A1A1A</item>
    </style>
</resources>